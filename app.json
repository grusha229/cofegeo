{
  "expo": {
    "name": "Favs: best places in Europe",
    "slug": "cofegeo",
    "scheme": "your-app-scheme",
    "version": "1.0.3",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "plugins": [
      "@react-native-google-signin/google-signin",
      "expo-router",
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Archivo-Medium.ttf"
          ]
        }
      ],
      "@react-native-firebase/app",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ],
      "expo-asset"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "website.favs.app",
      "googleServicesFile": "./src/utils/GoogleService-Info.plist",
      "infoPlist": {
        "NSPrivacyAccessedAPICategoryFileTimestamp": "3B52.1 - This app needs access to file timestamp information for synchronization purposes.",
        "NSPrivacyAccessedAPICategoryDiskSpace": "85F4.1 - This app needs access to disk space information for efficient storage management.",
        "NSPrivacyAccessedAPICategorySystemBootTime": "35F9.1 - This app needs access to system boot time information for performance optimization.",
        "NSPrivacyAccessedAPICategoryUserDefaults": "CA92.1 - This app needs access to user defaults for saving user preferences.",
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "com.googleusercontent.apps.1076432211496-a5kbtfng4f3sje3f82chpom0tli632qi"
            ]
          }
        ]
      },
      "privacyManifests": {
        "NSPrivacyAccessedAPITypes": [
          {
            "NSPrivacyAccessedAPIType": "NSPrivacyAccessedAPICategoryDiskSpace",
            "NSPrivacyAccessedAPITypeReasons": [
              "85F4.1"
            ]
          },
          {
            "NSPrivacyAccessedAPIType": "NSPrivacyAccessedAPICategoryFileTimestamp",
            "NSPrivacyAccessedAPITypeReasons": [
              "3B52.1"
            ]
          },
          {
            "NSPrivacyAccessedAPIType": "NSPrivacyAccessedAPICategorySystemBootTime",
            "NSPrivacyAccessedAPITypeReasons": [
              "35F9.1"
            ]
          },
          {
            "NSPrivacyAccessedAPIType": "NSPrivacyAccessedAPICategoryUserDefaults",
            "NSPrivacyAccessedAPITypeReasons": [
              "CA92.1"
            ]
          }
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.g.tupikov.cofegeo",
      "googleServicesFile": "./src/utils/GoogleService-Info.plist"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "43689992-519b-4088-a10c-16d4187ea3f2"
      }
    },
    "runtimeVersion": "1.0.0",
    "updates": {
      "url": "https://u.expo.dev/43689992-519b-4088-a10c-16d4187ea3f2"
